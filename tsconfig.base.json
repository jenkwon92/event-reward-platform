{
    "compilerOptions": {
      "target": "ES2021",
      "module": "commonjs",
      "strict": true,
      "strictPropertyInitialization": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "@common/*": ["libs/common/src/*"]
      }
    }
  }
  